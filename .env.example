# Copy this to .env and fill in your values

# Telegram Bot Token (get from @BotFather)
BOT_TOKEN=YOUR_BOT_TOKEN_HERE

# Your Telegram Channel ID (e.g., @MedicNEET or -1001234567890)
CHANNEL_ID=@yourchannel

# Google Sheet ID (the long string in the sheet URL)
GOOGLE_SHEET_ID=YOUR_SHEET_ID_HERE

# Path to Google Service Account credentials JSON
GOOGLE_CREDS_FILE=credentials.json

# Your deployed URL (e.g., https://quiz.medicneet.com)
WEBAPP_URL=https://yourdomain.com

# MedicNEET Play Store Link
PLAYSTORE_LINK=https://play.google.com/store/apps/details?id=com.medicneet.app
